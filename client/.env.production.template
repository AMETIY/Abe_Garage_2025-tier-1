# Production Environment Configuration for Vercel
# This file contains production-specific environment variables

# API Configuration
VITE_API_URL=https://abe-garage-backend.onrender.com

# Application Configuration
VITE_APP_NAME=Abe Garage Management System
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security Configuration
VITE_ENABLE_HTTPS_ONLY=true
VITE_ENABLE_STRICT_CSP=true

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_LAZY_LOADING=true
VITE_CHUNK_SIZE_WARNING_LIMIT=1000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=false
VITE_PAGINATION_DEFAULT_SIZE=10

# Development/Debug (disabled in production)
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false
VITE_ENABLE_REACT_DEVTOOLS=false